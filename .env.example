
# Região onde o bucket está hospedado (ajuste conforme sua escolha)
AWS_REGION=us-east-1

# Nome do bucket padrão S3
DEFAULT_BUCKET=chatbot-prompts

# Formato do path no S3: clienteID/clienteID-prompt.json
PROMPT_FILE_PATTERN=%s/%s-prompt.json

# Tempo de expiração da URL assinada (em minutos)
EXPIRATION_MINUTES=10

# Credenciais AWS (NUNCA versionar essas chaves!)
AWS_ACCESS_KEY_ID=SEU_ACCESS_KEY_ID
AWS_SECRET_ACCESS_KEY=SEU_SECRET_ACCESS_KEY

# Endpoint customizado (em branco para AWS S3 padrão)
AWS_ENDPOINT=